<!DOCTYPE html>
<html>
<head>
  <title>Experiencewithme</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class='notice'>
    <% if flash[:notice] %>
      <p class="fourcol"><%=h flash[:notice] %></p>
    <% end %>
  </div>
  <div class="container"> 
    <div class="row" "navbar"> 
      <%=render "shared/topnav" %> 
    </div> 
  </div> 
  <div class="container">
    <div class="row" "eightcol">
      <div id="calendar_container">
        <%= yield %>
      <div> 
    </div> 
  </div> 
</body>
</html>
